<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".EVA2.retrofit_listview.DetalleVolumen">

    <ImageView
        android:id="@+id/id_imagen_volumen"
        android:layout_width="80dp"
        android:layout_height="100dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/id_libro_nombre"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="nombre"
        tools:layout_editor_absoluteX="34dp"
        tools:layout_editor_absoluteY="195dp" />

    <TextView
        android:id="@+id/id_libro_desc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="desc"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="444dp" />

    <TextView
        android:id="@+id/id_fecha_publicacion"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="fecha"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="379dp" />

    <TextView
        android:id="@+id/id_libro_autor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="autor"
        tools:layout_editor_absoluteX="34dp"
        tools:layout_editor_absoluteY="310dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="subtitle"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="265dp" />
</androidx.constraintlayout.widget.ConstraintLayout>